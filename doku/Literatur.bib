@article{Pang2018,
abstract = {In der Literatur finden sich unterschied- liche Ans{\"{a}}tze zur Steuerung mehrgliedriger, schlangen{\"{a}}hnlicher Roboter mit angetriebenen R{\"{a}}dern, die sich in ihrer Komple- xit{\"{a}}t teils stark voneinander unterscheiden. In der vorliegenden Arbeit wird die Leistungsf{\"{a}}higkeit eines vergleichsweise einfa- chen, rein kinematischen n-Trailer Modells f{\"{u}}r die Steuerung eines solchen Roboters, der zuk{\"{u}}nftig zur Inspektion innerhalb eines Teilchenbeschleunigers eingesetzt werden soll, validiert. Bevor eine Herleitung des Modells erfolgt, wird zun{\"{a}}chst auf die technischen Besonderheiten und die Inbetriebnahme des verwendeten Roboters eingegangen. Anschlie{\ss}end werden die Ergebnisse aus Versuchsfahrten vorgestellt und die Leistungsf{\"{a}}- higkeit des Modells diskutiert. Es zeigt sich, dass sich f{\"{u}}r den hier vorliegenden Verwendungszweck bereits mit dem kinematischen n-Trailer Modell sehr gute Resultate erzielen lassen, weshalb auf eine komplexere Steuerung mit Ber{\"{u}}cksichtigung dynamischer Effekte unter Umst{\"{a}}nden verzichtet werden kann.},
author = {Pang, Zhichao and Raab, Sebastian and Tamm-morschel, Jonas},
file = {:home/jens/HESSENBOX-DA/PS{\_}WS1819 (Nicolai Schweizer)/literature/PS{\_}Robotik{\_}SS18.pdf:pdf},
title = {{Validierung eines kinematischen n -Trailer Modells f{\"{u}}r die Steuerung eines Schlangenroboters}},
year = {2018}
}
