@article{Imer2016,
abstract = {Diese Arbeit setzt sich mit der voll- automatisierten Kalibrierung einer inertialen Messeinheit (IMU1) f{\"{u}}r ein UAV2 auseinander. Intertiale Messeinheiten werden in gro{\ss}em Umfang in der Robotik eingesetzt. Jedoch sind unkali- brierte IMU's hier im Allgemeinen nicht zuverl{\"{a}}ssig anwendbar. Zun{\"{a}}chst werden die grundlegenden technischen Aspekte, wie die Funktionsweise einer IMU und die Fehlermodelle vorgestellt. Das Fehlermodell des Beschleunigungssensors ber{\"{u}}cksichtigt Skalie- rungsfehler und einen festen Bias, sowie Orthogonalit{\"{a}}tsfehler. Das Modell des Drehratengebers beschr{\"{a}}nkt sich auf einen temperaturabh{\"{a}}ngigen Bias. Durch die Methode der kleinsten Quadrate werden die Parameter der vorgestellten Fehlermodelle ermittelt. Ein einfacher Aufbau, mit dem die Messung durchgef{\"{u}}hrt wird, ist ebenfalls Bestandteil dieser Arbeit. Die Verbesserungen wer- den anhand von Messdaten quantitativ verglichen. Die Ergebnisse werden kritisch hinterfragt. Abschlie{\ss}end wird ein Ausblick auf Erweiterungen dieser Arbeit gegeben.},
author = {Imer, Selim and Kempkes, Moritz and Klein, Sebastian Rudolf and Schr{\"{o}}der, Maike},
file = {:home/jens/HESSENBOX-DA/PS{\_}WS1819 (Nicolai Schweizer)/literature/IMU{\_}Kalibrierung.pdf:pdf},
title = {{Kalibrierung einer Inertial Measurement Unit ( IMU ) f{\"{u}}r ein UAV}},
year = {2016}
}
